generator client {
    provider = "prisma-client-js"
    output   = "../prisma/generated/client"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Result {
    id         String @id @default(auto()) @map("_id") @db.ObjectId
    uid        String
    percentage Int
    createdAt  String
    role       String
    fullName   String
    photo      String
}
