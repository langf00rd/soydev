// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Result {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  uid        String
  percentage Int
  createdAt  String
  role       String
  fullName   String
  photo      String
}
